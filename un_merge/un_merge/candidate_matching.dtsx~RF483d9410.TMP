          SQLTask:Connection="{CA099C18-4884-47C3-BA4F-335965B85796}"
          SQLTask:SqlStatementSource="--Remove rows where match_text is too small&#xA;DELETE FROM ##uk_match_candidate_data&#xA;WHERE LEN(match_text) &lt; 2&#xA;  AND match_code IN(8, 9)&#xA;&#xA;DELETE FROM ##uk_match_candidate_data&#xA;WHERE LEN(match_text) &lt; 7&#xA;  AND match_code NOT IN(8, 9)&#xA;&#xA;--Remove duplcate match data for each person&#xA;;WITH cte AS (&#xA;SELECT ref, match_text, match_code, MIN(id) AS id&#xA;FROM ##uk_match_candidate_data&#xA;GROUP BY ref, match_text, match_code&#xA;HAVING COUNT(*) &gt; 1)&#xA;DELETE FROM a&#xA;FROM ##uk_match_candidate_data a&#xA;  INNER JOIN cte b&#xA;    ON a.ref = b.ref&#xA;   AND a.match_text = b.match_text&#xA;   AND a.match_code = b.match_code&#xA;WHERE a.id != b.id&#xA;&#xA;--Remove rows where match data is unique and therefore can't be  a match&#xA;;WITH cte AS (&#xA;SELECT &#xA;  match_code&#xA;  ,match_text &#xA;FROM ##uk_match_candidate_data&#xA;GROUP BY&#xA;  match_code&#xA;  ,match_text &#xA;HAVING COUNT(*) = 1&#xA;)&#xA;DELETE &#xA;FROM ##uk_match_candidate_data&#xA;WHERE EXISTS &#xA;  (SELECT 1 &#xA;   FROM cte &#xA;   WHERE cte.match_code = ##uk_match_candidate_data.match_code &#xA;     AND cte.match_text = ##uk_match_candidate_data.match_text) " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
