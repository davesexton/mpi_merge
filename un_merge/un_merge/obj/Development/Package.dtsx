      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2014; © 2014 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{DF2B02C5-883B-4BAA-B3D9-17A96036ABE2}"
          SQLTask:SqlStatementSource="DELETE&#xA;FROM organisation_map   &#xA;WHERE NOT EXISTS (    &#xA;SELECT 1&#xA;FROM organisation_map imap&#xA;  INNER JOIN organisation pp &#xA;    ON pp_organisation_ref = pp.organisation_ref&#xA;  INNER JOIN organisation mp &#xA;    ON mp_organisation_ref = mp.organisation_ref&#xA;WHERE pp.source = 'pp'   &#xA;  AND mp.source = 'mp'&#xA;  AND organisation_map.pp_organisation_ref = imap.pp_organisation_ref&#xA;  AND organisation_map.mp_organisation_ref = imap.mp_organisation_ref);&#xA;&#xA;DELETE&#xA;FROM opportunity_map   &#xA;WHERE NOT EXISTS (  &#xA;SELECT 1&#xA;FROM opportunity_map imap&#xA;  INNER JOIN opportunity pp &#xA;    ON pp_opportunity_ref = pp.opportunity_ref&#xA;  INNER JOIN opportunity mp &#xA;    ON mp_opportunity_ref = mp.opportunity_ref&#xA;WHERE pp.source = 'pp'   &#xA;  AND mp.source = 'mp'&#xA;  AND opportunity_map.pp_opportunity_ref = imap.pp_opportunity_ref&#xA;  AND opportunity_map.mp_opportunity_ref = imap.mp_opportunity_ref);&#xA;&#xA;WITH cte AS (&#xA;SELECT pp_person_ref, mp_person_ref&#xA;  ,RANK() OVER(PARTITION BY pp_person_ref ORDER BY CASE pp.first_name_denoise WHEN mp.first_name_denoise THEN 1 ELSE 0 END&#xA;    + CASE pp.last_name_denoise WHEN mp.last_name_denoise THEN 1 ELSE 0 END&#xA;    + CASE pp.email_address_denoise WHEN mp.email_address_denoise THEN 1 ELSE 0 END&#xA;    + CASE pp.create_timestamp WHEN mp.create_timestamp THEN 1 ELSE 0 END&#xA;    + CASE WHEN LEFT(pp.first_name_denoise, 2) = LEFT(mp.first_name_denoise, 2)&#xA;            AND LEFT(pp.last_name_denoise, 2) = LEFT(mp.last_name_denoise, 2)&#xA;           THEN 1 ELSE 0 END DESC) AS x_rank&#xA;FROM person_map imap&#xA;  INNER JOIN person pp &#xA;    ON pp_person_ref = pp.person_ref&#xA;  INNER JOIN person mp &#xA;    ON mp_person_ref = mp.person_ref&#xA;WHERE pp.source = 'pp'   &#xA;  AND mp.source = 'mp'&#xA;  AND CASE pp.first_name_denoise WHEN mp.first_name_denoise THEN 1 ELSE 0 END&#xA;    + CASE pp.last_name_denoise WHEN mp.last_name_denoise THEN 1 ELSE 0 END&#xA;    + CASE pp.email_address_denoise WHEN mp.email_address_denoise THEN 1 ELSE 0 END&#xA;    + CASE pp.create_timestamp WHEN mp.create_timestamp THEN 1 ELSE 0 END&#xA;    + CASE WHEN LEFT(pp.first_name_denoise, 2) = LEFT(mp.first_name_denoise, 2)&#xA;            AND LEFT(pp.last_name_denoise, 2) = LEFT(mp.last_name_denoise, 2)&#xA;           THEN 1 ELSE 0 END    &#xA;   &gt; 0)&#xA;DELETE&#xA;FROM person_map   &#xA;WHERE NOT EXISTS ( &#xA;  SELECT * &#xA;  FROM cte &#xA;  WHERE x_rank = 1   &#xA;    AND person_map.pp_person_ref = cte.pp_person_ref&#xA;    AND person_map.mp_person_ref = cte.mp_person_ref);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
