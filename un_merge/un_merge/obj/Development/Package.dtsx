                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
  opportunity_ref 
  ,organisation_ref 
  ,displayname
  ,CASE ISDATE(date_opened) WHEN 1 THEN CAST(date_opened AS DATETIME) END AS date_opened
  ,CASE ISDATE(date_closed) WHEN 1 THEN CAST(date_closed AS DATETIME) END AS date_closed
  ,CASE ISDATE(create_timestamp) WHEN 1 THEN CAST(create_timestamp AS DATETIME) END AS create_timestamp
  ,CASE ISDATE(update_timestamp) WHEN 1 THEN CAST(update_timestamp AS DATETIME) END AS update_timestamp
  ,has_other_activity
FROM OPENQUERY([P7AAUK-SYB], '
SELECT
  a.opportunity_ref 
  ,organisation_ref 
  ,displayname
  ,date_opened
  ,date_closed
  ,create_timestamp
  ,CASE WHEN update_timestamp &gt;= date_closed AND update_timestamp &gt;= z_last_action
        THEN update_timestamp
        WHEN date_closed &gt;= z_last_action
        THEN date_closed
        ELSE z_last_action
        END AS update_timestamp
  ,CASE WHEN b.opportunity_ref IS NULL THEN ''N'' ELSE ''Y'' END AS has_other_activity
FROM (
  SELECT
    opportunity_ref 
    ,organisation_ref 
    ,displayname
    ,ISNULL(CAST(date_opened AS VARCHAR(23)), '''') AS date_opened
    ,ISNULL(CAST(create_timestamp AS VARCHAR(23)), '''') AS create_timestamp
    ,ISNULL(CAST(update_timestamp AS VARCHAR(23)), '''') AS update_timestamp
    ,ISNULL(CAST(date_closed AS VARCHAR(23)), '''') AS date_closed
    ,ISNULL(CAST(z_last_action AS VARCHAR(23)), '''') AS z_last_action
  FROM opportunity) a
  LEFT OUTER JOIN
  (SELECT DISTINCT r.opportunity_ref
  FROM event_role er
    INNER JOIN event r ON er.event_ref = r.event_ref
