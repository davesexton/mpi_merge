HERE NULLIF(pp_event_role_organisation_ref, 0) IS NOT NULL&#xA;  AND NULLIF(mp_event_role_organisation_ref, 0) IS NOT NULL&#xA;UNION&#xA;SELECT DISTINCT&#xA;  pp_pos.organisation_ref&#xA;  ,mp_pos.organisation_ref&#xA;FROM master_map mm &#xA;  INNER JOIN position pp_pos ON mm.pp_person_ref = pp_pos.person_ref&#xA;  INNER JOIN position mp_pos ON mm.mp_person_ref = mp_pos.person_ref&#xA;WHERE pp_pos.create_timestamp = mp_pos.create_timestamp&#xA;  AND pp_pos.organisation_ref IS NOT NULL&#xA;  AND mp_pos.organisation_ref IS NOT NULL&#xA;UNION&#xA;SELECT &#xA;  pp.organisation_ref&#xA;  ,mp.organisation_ref&#xA;FROM organisation pp&#xA;  INNER JOIN organisation mp ON pp.organisation_ref = mp.external_ref &#xA;WHERE pp.source = 'pp'&#xA;  AND mp.source = 'mp'&#xA;  AND pp.create_timestamp = mp.create_timestamp ;&#xA;  &#xA;TRUNCATE TABLE opportunity_map;  &#xA;INSERT INTO opportunity_map&#xA;(pp_opportunity_ref ,mp_opportunity_ref)&#xA;SELECT DISTINCT&#xA;  pp_opportunity_ref&#xA;  ,mp_opportunity_ref&#xA;FROM master_map&#xA;WHERE NULLIF(pp_opportunity_ref, 0) IS NOT NULL&#xA;  AND NULLIF(mp_opportunity_ref, 0) IS NOT NULL;&#xA;&#xA;TRUNCATE TABLE purge_opportunity_map;  &#xA;INSERT INTO purge_opportunity_map&#xA;(pp_opportunity_ref ,mp_opportunity_ref)&#xA;SELECT a.* &#xA;FROM opportunity_map a&#xA;  INNER JOIN opportunity b ON a.pp_opportunity_ref = b.opportunity_ref &#xA;WHERE a.pp_opportunity_ref NOT IN(SELECT pp_opportunity_ref &#xA;  FROM pp_master_map &#xA;  WHERE pp_opportunity_ref IS NOT NULL&#xA;    AND is_invoice_event = 'Y')&#xA;  AND b.source = 'pp'&#xA;  AND has_other_activity = 'N';&#xA;&#xA;TRUNCATE TABLE position_map;  &#xA;INSERT INTO position_map&#xA;SELECT DISTINCT&#xA;  pp_pos.position_ref&#xA;  ,mp_pos.position_ref&#xA;FROM master_map mm &#xA;  INNER JOIN position pp_pos ON mm.pp_person_ref = pp_pos.person_ref&#xA;  INNER JOIN position mp_pos ON mm.mp_person_ref = mp_pos.person_ref&#xA;WHERE pp_pos.create_timestamp = mp_pos.create_timestamp&#xA;  AN