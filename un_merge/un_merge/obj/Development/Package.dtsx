                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH cte AS (
SELECT
  t.name AS table_name
  ,column_id 
  ,CAST(c.name AS VARCHAR(8000)) AS column_name 
FROM sys.tables t
  INNER JOIN sys.columns c ON t.object_id = c.object_id
WHERE t.name LIKE '%_map'
  AND column_id = 1
UNION ALL
SELECT
  t.name AS table_name
  ,c.column_id 
  ,CAST(e.column_name  + ',' + c.name AS VARCHAR(8000)) AS column_name 
FROM sys.tables t
  INNER JOIN sys.columns c ON t.object_id = c.object_id
  INNER JOIN cte e ON t.name = e.table_name 
WHERE c.column_id = e.column_id + 1 
)
