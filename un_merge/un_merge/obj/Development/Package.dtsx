          SQLTask:SqlStatementSource="TRUNCATE TABLE person_type_map;&#xA;INSERT INTO person_type_map&#xA;SELECT&#xA;  pp.person_ref AS pp_person_ref&#xA;  ,mp.person_ref AS mp_person_ref&#xA;FROM person_type pp&#xA;  INNER JOIN person_map pm ON pp.person_ref = pp_person_ref&#xA;  LEFT OUTER JOIN person_type mp ON mp_person_ref = mp.person_ref&#xA;    AND mp.source = 'mp'&#xA;WHERE pp.source = 'pp'&#xA;  AND (pp.type = mp.type OR&#xA;       pp.create_timestamp = mp.create_timestamp OR&#xA;       pp.update_timestamp = mp.update_timestamp);&#xA;&#xA;INSERT INTO activity&#xA;SELECT DISTINCT&#xA;  pp.person_ref&#xA;  ,'person'&#xA;  ,'person_type'&#xA;FROM person_type pp&#xA;  INNER JOIN person_map pm ON pp.person_ref = pp_person_ref&#xA;  LEFT OUTER JOIN person_type_map ON pp.person_type_ref = pp_person_type_ref&#xA;WHERE pp.source = 'pp'&#xA;  AND pp_person_type_ref IS NULL;&#xA;&#xA;TRUNCATE TABLE candidate_map;&#xA;INSERT INTO candidate_map&#xA;SELECT&#xA;  pp.candidate_ref AS pp_candidate_ref&#xA;  ,mp.candidate_ref AS mp_candidate_ref&#xA;FROM person_type pp&#xA;  INNER JOIN person_type_map pm ON pp.person_type_ref = pp_person_type_ref&#xA;  INNER JOIN person_type mp ON mp_person_type_ref = mp.person_type_ref&#xA;    AND mp.source = 'mp'&#xA;WHERE pp.source = 'pp';&#xA;&#xA;INSERT INTO activity&#xA;SELECT DISTINCT&#xA;  pp.person_ref&#xA;  ,'person'&#xA;  ,'candidate'&#xA;FROM person_type pp&#xA;  INNER JOIN person_type_map pm ON pp.person_type_ref = pp_person_type_ref&#xA;  LEFT OUTER JOIN candidate_map ON pp.candidate_ref = pp_candidate_ref&#xA;WHERE pp.source = 'pp'&#xA;  AND pp_candidate_ref IS NULL;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
