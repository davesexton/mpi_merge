              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; Â© Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="Get duplicates"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH cte AS (
SELECT
  MAX(ref) AS ref
  ,email_address
FROM ##uk_match_candidate
GROUP BY
    email_address
), main AS (
SELECT 
  c.email_address
  ,c.ref
  ,c.name
  ,m.ref AS match_ref
  ,m.name AS match_name
  ,CASE c.norm_name WHEN m.norm_name THEN 'Y' ELSE 'N' END AS match_full_name
  ,CASE CHARINDEX(c.norm_first_name, m.norm_first_name)
     + CHARINDEX(m.norm_first_name, c.norm_first_name)   
     WHEN 0 THEN 'N' ELSE 'Y' END AS match_first_name
  ,CASE CHARINDEX(c.norm_last_name, m.norm_last_name)
     + CHARINDEX(m.norm_last_name, c.norm_last_name)   
     WHEN 0 THEN 'N' ELSE 'Y' END AS match_last_name
  ,CASE WHEN c.date_of_birth IS NULL OR m.date_of_birth IS NULL
        THEN 'U'   
        WHEN c.date_of_birth = m.date_of_birth 
        THEN 'Y' 
        ELSE 'N' 
        END AS match_date_of_birth
  ,CASE WHEN c.mobile IS NULL OR m.mobile IS NULL 
        THEN 'U'
        WHEN c.mobile = m.mobile 
        THEN 'Y' 
        ELSE 'N' 
        END AS match_mobile
  ,CASE WHEN c.telephone IS NULL OR m.telephone IS NULL 
        THEN 'U'
        WHEN c.telephone = m.telephone 
        THEN 'Y' 
        ELSE 'N' 
        END AS match_telephone
  ,CASE WHEN c.postcode IS NULL OR m.postcode IS NULL 
        THEN 'U'
        WHEN c.postcode = m.postcode 
        THEN 'Y' 
        ELSE 'N' 
        END AS match_postcode
  ,CASE c.norm_name WHEN m.norm_name THEN 1 ELSE 0 END
    + CASE CHARINDEX(c.norm_first_name, m.norm_first_name)
         + CHARINDEX(m.norm_first_name, c.norm_first_name)   
        WHEN 0 THEN 0 ELSE 1 END
    + CASE CHARINDEX(c.norm_last_name, m.norm_last_name)
         + CHARINDEX(m.norm_last_name, c.norm_last_name)   
        WHEN 0 THEN 0 ELSE 1 END
    + CASE WHEN c.date_of_birth IS NULL OR m.date_of_birth IS NULL
          THEN 0  
          WHEN c.date_of_birth = m.date_of_birth 
          THEN 1 
          ELSE -1 
          END
    + CASE WHEN c.mobile IS NULL OR m.mobile IS NULL 
        THEN 0
        WHEN c.mobile = m.mobile 
        THEN 1 
        ELSE -1 
        END
    + CASE WHEN c.telephone IS NULL OR m.telephone IS NULL 
           THEN 0
           WHEN c.telephone = m.telephone 
           THEN 1 
           ELSE -1 
           END
    + CASE WHEN c.postcode IS NULL OR m.postcode IS NULL 
           THEN 0
           WHEN c.postcode = m.postcode 
           THEN 1 
           ELSE -1 
           END AS score
FROM cte cc
  INNER JOIN ##uk_match_candidate c ON c.ref = cc.ref
  INNER JOIN ##uk_match_candidate m ON m.email_address = cc.email_address
WHERE cc.ref != m.ref
  AND c.gender = m.gender)
SELECT * 
FROM main
WHERE score &gt;= 0
ORDER BY score DESC</property>
