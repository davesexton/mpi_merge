      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{CA099C18-4884-47C3-BA4F-335965B85796}"
          SQLTask:SqlStatementSource=";WITH cte AS (&#xA;SELECT &#xA;  norm_first_name&#xA;  ,CASE WHEN SUM(CASE gender WHEN 'F' THEN 1 ELSE 0 END) &gt; SUM(CASE gender WHEN 'M' THEN 1 ELSE 0 END)&#xA;        THEN  'F' ELSE 'M' END AS gender&#xA;FROM ##uk_match_candidate&#xA;WHERE gender IS NOT NULL&#xA;GROUP BY norm_first_name&#xA;)&#xA;UPDATE md&#xA;SET gender = cte.gender&#xA;FROM ##uk_match_candidate md&#xA;  INNER JOIN cte ON md.norm_first_name = cte.norm_first_name&#xA;WHERE md.gender IS NULL" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
