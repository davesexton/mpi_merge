            <component
              refId="Package\Load pp_master_map\Get PP data"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; Â© Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="Get PP data"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
  UPPER(CAST(master.dbo.fn_varbintohexsubstring(0, HASHBYTES('SHA1', 
    CONVERT(VARCHAR, create_timestamp, 121)
      + ISNULL(CONVERT(VARCHAR, event_date, 121), '')
      + ISNULL(CONVERT(VARCHAR, event_time, 121), '')
      + CASE e_type 
          WHEN 'P031' THEN 'N231'
          WHEN 'P061' THEN 'N25'
          WHEN 'P90' THEN 'N30'
          ELSE ISNULL(e_type, '') END
      + ISNULL(notes, '')
      + CASE event_ref WHEN master_event_ref THEN 'Y' ELSE 'N' END
      + ISNULL(er_type, '')
      + CAST(RANK() OVER(PARTITION BY event_ref, er_type 
                         ORDER BY event_role_ref ) AS VARCHAR) 
    ), 1, 0) AS VARCHAR)) AS pp_hash_code
  ,event_ref AS pp_event_ref
  ,event_role_ref AS pp_event_role_ref
  ,opportunity_ref AS pp_opportunity_ref
  ,e_organisation_ref AS pp_event_organisation_ref
  ,er_organisation_ref AS pp_event_role_organisation_ref
  ,person_ref AS pp_person_ref
  ,create_timestamp
FROM OPENQUERY([P7AAUK-SYB], '
SELECT 
  e.create_timestamp
  ,e.event_date
  ,e.event_time
  ,e.type AS e_type 
  ,e.notes
  ,e.event_ref
  ,e.master_event_ref
  ,er.type AS er_type
  ,er.event_role_ref
  ,e.opportunity_ref
  ,e.organisation_ref AS e_organisation_ref
  ,er.organisation_ref AS er_organisation_ref
  ,er.person_ref AS person_ref
FROM event_role er 
  INNER JOIN event e ON er.event_ref = e.event_ref
WHERE EXISTS 
  (SELECT 1
   FROM event_role ier 
   WHERE ier.team IN (''DBRM'', ''DCPL'', ''DCTL'', ''DDSL'', ''DSEL'', ''DSTL'', 
                      ''DWBS'', ''DWEL'', ''DWPL'', ''DWTL'', ''TDBL'', ''TDBR'',
                      ''TDCR'', ''TDEL'', ''TDMK'', ''TDMS'', ''TDRD'', ''TDSL'',
                      ''THLN'', ''THSA'', ''DDCR'', ''DDEL'', ''DDMK'', ''DDRD'',
                      ''DHLN'', ''DDXC'')
     AND er.event_ref = ier.event_ref)
')</property>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
