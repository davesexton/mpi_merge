  ,mp.person_ref AS mp_person_ref
  ,mp.name AS mp_name
FROM person_pp_matchdata pp
  INNER JOIN person_mp_matchdata mp ON pp.matchname = mp.matchname
WHERE (CASE pp.matchtext01 WHEN mp.matchtext01 THEN 2 ELSE 0 END
