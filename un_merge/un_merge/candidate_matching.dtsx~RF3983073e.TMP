              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT   
  person_ref AS ref
  ,UPPER(email_address) AS match_text
FROM uk.person
WHERE person_ref IN(SELECT person_ref FROM ##uk_match_candidate)
  AND LEN(email_address) &gt; 2
  AND email_address LIKE '%@%'
  AND email_address NOT LIKE '%@broadbean.net%'
  AND email_address NOT LIKE '%@pagepersonnel.co.uk%'
  AND email_address NOT LIKE '%@michaelpage.com%'</property>
