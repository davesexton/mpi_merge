                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">;WITH cte AS (
SELECT 
  organisation_ref AS ref
  ,name
FROM uk.organisation
WHERE name IS NOT NULL
  AND name = displayname
UNION ALL
SELECT 
  organisation_ref AS ref
  ,displayname
FROM uk.organisation
WHERE name IS NOT NULL
  AND name != displayname)
SELECT o.ref, UPPER(o.name) AS name, a.zipcode
FROM cte o
  LEFT OUTER JOIN uk.address a ON o.ref = a.organisation_ref
    AND a.main_address = 'Y'</property>
