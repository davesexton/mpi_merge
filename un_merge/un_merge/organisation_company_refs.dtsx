                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  ref
  ,name
  ,company_name AS companies_house_current_name
  ,NULL AS companies_house_previous_name
  ,c.company_number AS companies_house_number
FROM organisations o
  INNER JOIN companies_house_companies c ON o.name_norm = c.company_name_norm
